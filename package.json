{
  "name": "daily-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "svg": "vsvg -s ./icons -t ./src/icons"
  },
  "dependencies": {
    "intersection-observer": "^0.5.0",
    "localforage": "^1.7.2",
    "mixpanel-browser": "^2.22.4",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-svgicon": "^3.2.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-airbnb": "^3.0.4",
    "postcss-extend": "^1.0.5",
    "postcss-preset-env": "^6.0.7",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-extend": {},
      "postcss-preset-env": {
        "stage": 1,
        "features": {
          "nesting-rules": true,
          "custom-media-queries": {
            "extensions": {
              "--mobile-m": "(min-width: 375px)"
            }
          }
        }
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
